    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.10",
    "jest": "^26.5.3",
    "nodemon": "^2.0.4",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@tsed/exceptions": "^6.17.1",
    "@types/cors": "^2.8.5",
    "argon2": "^0.27.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parse": "^4.12.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonschema": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "pg": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "typeorm": "^0.2.28",
    "yup": "^0.32.8"
  }
}
